rule plink_make_bed:
    input:
        plinkIn + '.ped',
        plinkIn + '.map'
    params:
        outProj = 'plink_start/samples'
    output:
        'plink_start/samples.bed',
        'plink_start/samples.bim',
        'plink_start/samples.fam'
    shell:
        'module load plink2/1.90b3.32;plink --file ' + plinkIn + ' --make-bed --out {params.outProj}'

